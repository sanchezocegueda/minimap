# Correct clock rate for serial output
CONFIG_XTAL_FREQ_26=y

# GPS Config
CONFIG_NMEA_PARSER_UART_RXD=33
CONFIG_NMEA_STATEMENT_GGA=y
CONFIG_NMEA_STATEMENT_GSA=y
CONFIG_NMEA_STATEMENT_GSV=y
CONFIG_NMEA_STATEMENT_RMC=y
CONFIG_NMEA_STATEMENT_GLL=n
CONFIG_NMEA_STATEMENT_VTG=n

# Lora
CONFIG_LORA_CS_GPIO=13
# Change RST to esp RST
CONFIG_LORA_RST_GPIO=35
CONFIG_LORA_MISO_GPIO=14
CONFIG_LORA_MOSI_GPIO=12
CONFIG_LORA_SCK_GPIO=27
# LoRa Interrupts
CONFIG_LORA_DIO0_GPIO=26
CONFIG_LORA_IRQ_ENABLED=y
